{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "denatural",
  "patterns": [
    {
      "include": "#core"
    }
  ],
  "repository": {
    "core": {
      "patterns": [
        {
          "include": "#keywords"
        },
        {
          "include": "#plane-dividers"
        },
        {
          "include": "#embedded-languages"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.denatural",
          "match": "\\b(log)\\b"
        }
      ]
    }
  },
  "scopeName": "source.plurid"
}
